#!/bin/bash

source $HOME/.config/morevna-studio/morevna-studio.conf

if [ "$USE_0INSTALL" == "0" ]; then
    SCRIPTPATH="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
    "${SCRIPTPATH}/../lib/morevna-studio/morevna-studio-open" "$@"
    #"${MOREVNASTUDIO_LAUNCHER}-open" "$@"
else
    0install run --command=morevna-studio-open https://.../morevna-studio.xml "$@"
fi
